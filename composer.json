{
    "name": "dicr/yii2-cdek",
    "type": "yii2-extension",
    "description": "API службы доставки СДЭК",
    "license": "Apache-2.0",
    "homepage": "https://gitlab.com/dicr/yii2-cdek",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-cdek.git"
    },
    "keywords": [
        "dicr", "php", "yii2", "cdek"
    ],
    "autoload": {
        "psr-4": {
            "dicr\\cdek\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "php": "~7.3",
        "ext-zlib": "*",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-httpclient": "~2.0",

        "dicr/yii2-http": "~2.1",
        "dicr/yii2-json": "~2.0",
        "dicr/yii2-validate": "~2.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~9.5"
    },
    "repositories": [
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-http.git"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-json.git"},
        {"type": "git", "url": "https://gitlab.com/dicr/yii2-validate.git"},
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
